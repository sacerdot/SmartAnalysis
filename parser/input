Contract c (10) {   (* 10 ̀e il balance iniziale *)

    int weight = 2
    Contract p

    function foo(int r, bool b) : int
    {
        weight = 2
        weight = 3
        return weight
    }
    function fie() : string
    {
        return "Catched!"
    }
}
Contract d (3) {   (* 3 ̀e il balance iniziale *)
    
    function loop() : int
    {
        return 5
    }

    function foo(string s, Contract con) : int
    {
        int r
        r = max (8,9)
        r = symbol ("a")
        r = loop()
        r = c.foo (s, this)
        return r
    }
}
