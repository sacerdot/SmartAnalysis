Contract c {

    int balance = 0
    int weight = 2
    int retval = 0
    bool cond = true
    Contract p = banca
    Contract r = cianca 
    dep: int*bool -> int =
    fun x b -> 
        p = r
        balance = max 5*weight 4
        if  x == 0 then
            weight = weight - 1
        else
            balance = 1
    return 1 

    foo: string -> int =
    fun s ->
        s = "ciao"
        cond = (retval == 9)
        retval = dep 4 true
    return retval 
}
