Contract c (10) {   (* 10 ̀e il balance iniziale *)

    int weight = 2
    Contract p
    Contract q

    foo: string*Contract ->  int =
    fun s k ->
        int retval
        this.q = d
        retval = (k.dep.(value 4) 4 true p q)
