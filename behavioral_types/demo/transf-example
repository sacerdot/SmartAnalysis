contract a {

    int x = 0

    function addbalance () : int {
        return value
    }

    function getbalance () : int {
        return balance
    }

    function transf_tob (int v) : int {
        
        if (v <= balance) then 
            x = b.addbalance.value(v)()
        else 
            x = 0
        return x
    }
}

contract b {


    int x = 0
    function addbalance () : int {
        return value
    }

    function getbalance () : int {
        return balance
    }

    function transf_toa (int v) : int {
        int x = 0
        if (v <= balance) then 
               x = a.addbalance.value(v)()
        else 
            x = 0
        return x
    }

}
